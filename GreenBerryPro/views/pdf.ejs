		<style>
		body {
			position: fixed;
			width: 100%;
			height: 100%;
			margin: 0;
		}
		object {
			position: relative;
			width: 100%;
			height: 100%;
		}
		</style>
	</head>
	<body>
		<object id="embedded" data="/pdfFile/Hello" type="application/pdf">
